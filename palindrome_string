import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	   String s="madam";
	   String rev="";
	   for(int i=s.length()-1;i>=0;i--){
	       rev=rev+s.charAt(i);
	   }
	   if(s.equals(rev)){
	       System.out.print("is a palindrome");
	   }
	   else{
	       System.out.print("not a palindrome");
	   }
	    
		
}
}
